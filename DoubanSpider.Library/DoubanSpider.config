<?xml version="1.0" encoding="utf-8"?>
<DoubanSpider xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!--代理服务器列表-->
  <!--<ProxyList>61.93.246.50:8080;218.57.200.15:8080</ProxyList>-->
  <ProxyList></ProxyList>
  <!--获取代理服务器Url-->
  <ProxyUrl></ProxyUrl>
  <!--并发线程数-->
  <ThreadNum>1</ThreadNum>
  <!--Http请求时限ms-->
  <TimeLimit>1000</TimeLimit>
  <!--没有proxy的情况下自动延迟-->
  <AutoDelay>true</AutoDelay>
</DoubanSpider>